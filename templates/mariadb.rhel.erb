[mariadb]
name = MariaDB
baseurl = <%= node['maria']['repo'] %>/<%= node['maria']['version'] %><%= node['maria']['family'] %><%= node[:platform] == "redhat" ? node[:platform_family] : node[:platform] %>/$releasever/$basearch
gpgkey=<%= node['maria']['repo_key'] %>/<%= node['maria']['repo_name'] %>
gpgcheck=1
